{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>renovatebot/.github"
  ],
  "prCreation": "immediate",
  "prConcurrentLimit": 5,
  "packageRules": [
    {
      "enabled": true,
      "automerge": false,
      "matchPackageNames": [
        "*"
      ]
    }
  ],
  "npm": {
    "enabled": true,
    "automerge": false
  }
}
