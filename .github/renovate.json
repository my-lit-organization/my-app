{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "automerge": false,
  "extends": [
    "config:recommended",
    ":disableDependencyDashboard"
    ],
  "useBaseBranchConfig": "merge",
  "baseBranches": ["feat/renovate"],
  "packageRules": [
    {
        "matchDepNames": ["express"], 
        "enabled": true
    }
  ], 
  "vulnerabilityAlerts": {
    "labels": ["security"],
    "automerge": false,
    "enabled": true
  }
}
